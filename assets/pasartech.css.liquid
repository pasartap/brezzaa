/*
* PasarTap CSS customization
*/

#header {
    background-image: none !important;
}

.site-nav::before,
.top-nav::before,
.site-nav::after {
    display: none !important;
}

.site-nav-dropdown {
    overflow-y: auto;
    max-height: 600px;
}

#sticky-mmenu {
    position: fixed;
    width: 100%;
    top: 0;
}

nav#sticky-menu:not( .mm-menu),
nav#sticky-cart:not( .mm-menu) {
    display: none;
}

.mh-head .mh-btns-right {
    right: 0px;
}

.mm-navbars-top {

    border: none;
    top: 0;
    height: auto;
}

.mm-navbar-size-2 {
    height: 60px !important;
}

.mm-hasnavbar-top-1 .mm-panels {
    top: 46px;
}

.no-spend-notification {
    top: 46px !important;
}

.has-spend-notification {
    top: 82px !important;
}

.mm-hasnavbar-top-2 .mm-panels {
    top: 60px !important;
}

.mm-menu {
    font-family: "Open Sans", sans-serif;
}

.mm-panels .sca-qv-image {
    position: inherit;
}

.summary-mobile {
    float: right;
    width: 100%;
    font-size: 18px;
    font-weight: bold;
    margin-bottom: 10px;
}

#logo {
    margin-bottom: 0px;
    margin-top: 22px;
}

#search-field,
.search-btn-bg,
.mgk-top-cart .wishlist a,
#cartToggle {
    border-radius: 6px;
    border: 2px solid #e0e0e0;
    height: 44px;
}

.sticky-cart-total {
    color: white !important;
    font-size: 18px !important;
    font-weight: bold !important;
    margin-right: 10px;
}

.fa-chevron-left {
    margin-left: 10px;
}

.fa-shopping-cart {
    margin-top: -3px;
}

.mgk-top-cart .wishlist a {
    color: #50a746;
}

.wishlist {
    float: right;
    margin-right: 10px;
}

.search-btn-bg {
    border-left: 1px #f1f1f1 solid !important;
    border-top-left-radius: 0px !important;
    border-bottom-left-radius: 0px !important;
}

.search-section {
    margin-left: 15px;
}

.sticky-header {
    margin-top: 68px !important;
    top: 0px !important;
}

.sticky-topbar {
    position: fixed;
    height: 70px;
    z-index: 1000;
    display: block;
    top: 0px;
    background: #50a746;
    width: 100%;
    transition: all 0.5s;
    border-bottom: 3px solid #50a746;
}

.sticky-user {
    margin-right: 170px;
}

.site-nav {
    text-align: center;
}

.top-nav::before {
    width: 0;
    height: 0;
    border-right: 25px solid #50a746;
    border-top: 27px solid transparent;
    border-bottom: 27px solid transparent;
    content: "";
    position: absolute;
    margin-left: -24px;
}

.site-nav>li:first-child>a>span {
    color: yellow;
}

.site-nav-dropdown .col-1 {
    width: 100%;
}

.top-cart-contain.sticky-topcart {
    top: 25px;
}

#mob-menu {
    margin: 0px;
    position: fixed;
    top: 0;
}

.cart-info {
    margin: 10px 10px 0;
    font-size: 10px;
}

. .cart_instructions {

    margin: 0 auto 10px;
}

#dropdown-cart {
    top: 42px;
    width: 400px;
    border-radius: 6px;
    border: 2px solid #e0e0e0;
    padding: 0;
    overflow-y: auto;
    max-height: 600px;
}

#dropdown-cart .summary {
    color: #50a746;
    font-size: 16px;
    font-weight: 700;
    font-family: 'Open Sans', sans-serif;
}

.mini-products-list .item {
    padding: 0;
    margin-bottom: 10px;
}

textarea {
    min-height: 20px;
}

.btn-checkout-mobile {
    background: #50a746;
    height: 40px;
    border: none;
    color: #fff;
    font-family: "Open Sans", sans-serif;
    font-size: 13px;
    font-weight: bold;
}

.add-to-cart-quantity-cart,
.add-to-cart-quantity {
    font-weight: bold;
    font-size: 14px;
    color: #50a746;
}

.add-to-cart-quantity-cart,
.add-to-cart-quantity,
.btn-remove-from-cart,
.btn-add-to-cart,
.btn-remove-cart,
.btn-add-cart {
    border: none !important;
    height: 32px;
    width: 36px;
    display: inline-block;
    text-align: center;
}

.add-to-cart-quantity-cart,
.btn-remove-cart,
.btn-add-cart {
    height: 24px;
    width: 28px;
}

.btn-remove-cart,
.btn-add-cart {
    margin: 0;
    padding: 0;
}

.sca-qv-button,
.btn-remove-from-cart,
.btn-add-to-cart,
.btn-remove-cart,
.btn-add-cart {
    color: white !important;
    background-color: #50a746 !important;
}

.cart-buttons-in-cart {
    display: inline-block;
    width: 68%;
    margin-bottom: 5px;
}

.cart-line-item-price {
    float: right;
    margin-top: 9px;
    margin-right: 3px;
    display: inline-block;
}

.mh-head {
    background-color: #50a746 !important;
}

.mobile-shadow {
    background-color: #50a746 !important;
    box-shadow: 0 4px 2px -2px rgba(0, 0, 0, 0.16);
}

.mobile-cart-close {
    float: left;
    font-size: 2em;
    color: white !important;
}

.tile_cart .mini-products-list a {
    min-height: 32px;
}

.notification-badge {
    position: absolute;
    top: 6px;
    left: 14px;

    background-color: rgba(212, 19, 13, 1);
    color: #fff;
    border-radius: 3px;
    padding: 1px 3px;
    font: 8px Open Sans;
}

.spend-notification {
    padding-top: 10px;
    text-align: center;
    background-color: rgb(247, 247, 247);
}

.remaining-amount {
    color: rgb(230, 61, 0);
    font-weight: 600;
}

.order-minimum {
    font-weight: 600;
}

button:disabled,
button[disabled] {
    background-color: #dddddd !important;
}

#main_content .product .product_price .money {
    padding: 0 !important;
}

.sca-qv-button {
    width: 120px !important;
}

.bestsell-pro .block-content {
    padding: 0px;
}

.featured-pro .block-title {
    border-bottom: none;
}

.featured-pro .owl-theme .owl-controls {
    right: -6.5%;
    top: 25%;
}

#bottom .social-links h3 {
    margin-top: 15px;
}

#footer .social-links ul {
    margin-top: 7px;
}

.section-header {
    margin-top: 15px;
}

.product_wrap #purchase {
    border: none;
}

.product_wrap .product_details {
    margin: 20px 0;
}

.product_wrap .product_details strong {
    width: 80px;
}

.margin-top-3px {
    margin-top: 3px;
}

.empty-cart-icon {
    width: 100px;
    height: 100px;
    margin: 20px 0;
}

.empty-cart-text {
    margin-bottom: 20px !important;
}

.no-items {
    text-align: center;
}


#main_content .product {
    overflow: hidden;
}

#main_content .product.sca-qv-image .sca-qv-button,
.hot-deal.sca-qv-image .sca-qv-button,
.owl-carousel .owl-item.sca-qv-image .sca-qv-button,
#best-seller-block .owl-carousel .owl-item.sca-qv-image .sca-qv-button {
    border-radius: 0;
    display: inherit !important;
    height: 38px !important;
    left: 102px !important;
    line-height: 16px !important;
    padding: 10px 0 6px !important;
    position: absolute !important;
    top: 61px !important;
    width: 34px !important;
    right: -50px;
    font-family: "Open Sans", sans-serif !important;
    font-size: 12px !important;
    font-weight: bold !important;
    text-transform: uppercase;
    transition: all 0.3s ease-out !important;
    -webkit-transition: all 0.3s ease-out !important;
    -moz-transition: all 0.3s ease-out !important;
    -ms-transition: all 0.3s ease-out !important;
    -o-transition: all 0.3s ease-out !important;
    right: inherit !important;
    z-index: 10 !important;
}

#main_content .product.sca-qv-image .sca-qv-button {
    border-radius: 0;
    display: inherit !important;
    height: 38px !important;
    left: 81px !important;
    line-height: 16px !important;
    padding: 10px 0 6px !important;
    position: absolute !important;
    top: 79px !important;
}

#main_content .product.sca-qv-image .sca-qv-button:before,
.owl-carousel .owl-item.sca-qv-image .sca-qv-button:before,
.hot-deal.sca-qv-image .sca-qv-button:before,
#best-seller-block .owl-carousel .owl-item.sca-qv-image .sca-qv-button:before {
    content: "\f002";
    font-family: 'FontAwesome';
    font-size: 15px;
    margin-right: 1px;
    font-weight: normal;
}

#main_content .product.sca-qv-image:hover .sca-qv-button {
    right: 0px;
    transition: all 0.3s ease-in !important;
    -webkit-transition: all 0.3s ease-in !important;
    -moz-transition: all 0.3s ease-in !important;
    -ms-transition: all 0.3s ease-in !important;
    -o-transition: all 0.3s ease-in !important;
}

#main_content .product:hover .box-hover {
    top: 0px;
  z-index: 10;
   
}

#main_content .product:hover .btn.btn-primary,
.item-inner:hover .btn.btn-primary{
    background: {{ settings.btn_primary_color_hover }};  
    color: {{ settings.btn_txt_color_hover }} !important;
    border: 1px {{ settings.btn_border_color_hover }} solid;
}

.slider-items-products .item {
    text-align: center;
    margin-top: 15px!important
}

#main_content .grid-uniform .product {
    margin: 0;
}

/* Responsive CSS */

@media only screen and (min-width: 320px) and (max-width: 479px) {
    #main_content .product.col-sm-4 {
        width: 48% !important;
    }

    .owl-item .item {
        margin: 10px 0 5px !important;
    }

    .bestsell-pro .block-title {
        height: inherit;
        padding-right: 60px;
    }

    .bestsell-pro .block-title h2 {
        font-size: 18px;
        padding-bottom: 6px;
    }

    .bestsell-pro .slider-items-products .owl-buttons .owl-next {
        right: -45px;
        top: -8px;
    }

    .bestsell-pro .slider-items-products .owl-buttons .owl-prev {
        left: 12px;
        top: -8px;
    }

    .featured-pro .slider-items-products .owl-buttons .owl-next {
        right: 20px;
    }

    .featured-pro .slider-items-products .owl-buttons .owl-prev {
        left: -45px;
    }

    .featured-block .item-inner {
        margin: 0 5px;
    }

    #main_content .grid-uniform .product {
        margin: 0;
        width: 48%;
    }
}

@media only screen and (min-width: 480px) and (max-width: 767px) {
    #main_content .product.col-sm-4 {
        width: 48%;
    }

    #main_content .grid-uniform .product {
        margin: 0;
        width: 31%;
    }
}

@media (min-width: 768px) and (max-width: 979px) {
    #main_content .product.col-sm-4 {
        width: 31%;
    }
}

@media(min-width: 768px) {
    .mh-head,
    #sticky-mmenu {
        display: none;
    }
}

@media only screen and (min-width: 978px) and (max-width:1259px) {
    .featured-pro .owl-theme .owl-controls {
        right: -8.5%;
        top: 32%;
    }

    .tile_search input {
        width: 355px;
    }
}

@media only screen and (max-width:767px) {
    .mini-products-list {
        display: inline;
    }

    .fa-shopping-cart {
        color: white;
        margin-top: 2px !important;
    }

    .cart-info {
        margin: 0;
    }

    .cart-buttons-in-cart {
        width: auto;
    }

    .money {
        font-weight: bold;
    }

    .header-top,
    #top-bar,
    #sticky-topbar {
        display: none !important;
    }

    .section-header {
        margin-top: 0;
    }

    .mm-page {
        margin-top: 80px !important;
    }

    .image-item {
        margin-top: 20px !important;
    }

    #main_content .product.col-sm-4 {
        width: 23%;
    }

    .grid-uniform>[class*="col-sm-4"]:nth-child(3n+1) {
        clear: inherit;
    }
}